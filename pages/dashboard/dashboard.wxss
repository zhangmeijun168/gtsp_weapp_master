/**index.wxss**/
.container {
  margin: 0 15px;
}

.header {
  background-color: #FFFFFF;
  height: 130px;
  box-shadow: 0 2px 4px rgba(140, 140, 140, 0.5);
  border-radius: 5px;
  display: flex;
  flex-direction: column;
  position: relative;
  margin-top: 8px;
}

.header-form {
  display: flex;
  align-items: flex-start;
}

.avatar {
  width: 50px;
  height: 50px;
  margin: 15px;
}

.user-info {
  padding-top: 10px;
  gap: 2px;
  display: flex;
  flex-direction: column;
}

.header-form .desc {
  /* padding-top: 4px; */
  color: #888888;
  font-size: small;
}

.header-form .station {
  /* padding-top: 4px; */
  color: #888888;
  font-size: small;
  text-decoration: underline;
}

.logo-backupgroud {
  position: absolute;
  height: 100px;
  width: 100px;
  right: 15px;
  top: 15px;
  opacity: 0.1;
}

.btn-area {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}

.btn-area-view {
  width: calc(33% - 7.5px);
  height: 125px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 3px;
  box-shadow: 0 2px 4px rgba(140, 140, 140, 0.5);
  margin-top: 15px;
}

.i-icon {
  display: inline-block;
  width: 45px;
  height: 45px;
  background-size: cover;
}

.icon-label {
  color: #555555;
  margin-top: 10px;
}

