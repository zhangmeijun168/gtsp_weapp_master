.tabButton {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 48%;
    border-right: solid 1px #13c2c2;
    border-top: solid 1px #13c2c2;
    border-bottom: solid 1px #13c2c2;
    /* font-size: 13px; */
    height: 30px;
}

.tabButton.first {
    border-radius: 5px 0 0 5px;
    border-left: solid 1px #13c2c2;
    margin-left: 8px;
}

.tabButton.last {
    border-radius: 0 5px 5px 0;
    margin-right: 8px;
}

.tabButton.check {
    background-color: #13c2c2;
    color: white
}

