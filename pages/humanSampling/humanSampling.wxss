.fix-view {
  position: fixed;
  z-index: 1000;
  bottom: 80px;
  right: 10px;
}

.circle {
  width: 54px;
  height: 54px;
  border-radius: 54px;
  position: relative;
  margin-top: 12px;
  color: white;
}

.mixNum-circle {
  background-color: #c1621ac4;
}

.mixNum-cur {
  position: absolute;
  top: 2px;
  text-align: center;
  width: 100%;
}

.mixNum-slash {
  position: absolute;
  top: 49%;
  left: 15%;
  width: 70%;
  border-top: 2px solid;
}

.mixNum-max {
  position: absolute;
  bottom: 2px;
  text-align: center;
  width: 100%
}

.paidMarker {
  border: 1px solid #5abfc0;
  padding: 10px 20px;
  color: #5abfc0;
  font-size: 18px;
  position: absolute;
  right: 10px;
  top: 30px;
  transform: rotate(30deg);
  opacity: 0.7;
}

.unPaidMarker {
  border: 1px solid red;
  padding: 10px 20px;
  color: red;
  font-size: 18px;
  position: absolute;
  right: 10px;
  top: 30px;
  transform: rotate(30deg);
  opacity: 0.7;
}

.dangerMarker {
  border: 1px solid red;
  padding: 10px 20px;
  color: red;
  font-size: 18px;
  position: absolute;
  right: 10px;
  top: 90px;
  transform: rotate(30deg);
  opacity: 0.7;
}

.info-box {
  border: 2px solid #13c2c2;
  padding: 10px;
  border-radius: 8px;
  background-color: white;
  margin-bottom: 10px;
}

.flex {
  display: flex;
  margin-top: 4px;
}

.info-label {
  width: 120px;
  margin-right: 20px;
  text-align: right;
}

